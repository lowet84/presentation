  "main": "src/index.ts",
  "scripts": {
    "debug": "tsc && npm run copy && node out/generator.js && node --inspect out/index.js dummy",
    "start": "tsc && npm run copy && concurrently \"tsc --watch\" \"node out/generator.js && nodemon --inspect out/index.js dummy\"",
