    "routing-controllers-openapi": "^1.4.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^10.14.1",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "koa-bodyparser": "^4.2.1",
    "nodemon": "^1.18.9",
    "parcel": "^2.8.2",
    "typescript": "^3.2.4"
